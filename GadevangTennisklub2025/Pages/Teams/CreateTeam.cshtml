    </div>
    <div class="form-group">
        <label asp-for="@Model.MembershipType" class="control-label"></label>
        <input asp-for="@Model.MembershipType" style="display: br; width: 30%;" class="form-control" />
    </div>



    <div class="formgroup">
        <form style="display: inline;" asp-page-handler="Create" asp-route-id="0">
